<div class="row">
    <div class="col-6">
        <div class="community__form">
            <%= form_for @community do |form| %>
            <div class="form-group">
                <%= label_tag :name %>
                <%= form.text_field :name, class: "form-control" %>
            </div>

            <%= form.submit "Submit", class: "btn btn-success"%>

            <% end %>
        </div>
    </div>
    <div></div>
</div>